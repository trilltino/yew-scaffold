{
  "buildCommand": "trunk build --release --no-sri",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/yew-scaffold/**/*.js",
      "headers": [
        {
          "key": "Content-Type",s
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/yew-scaffold/**/*.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/yew-scaffold/(.*\\.(js|wasm|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/yew-scaffold/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/yew-scaffold/(.*)",
      "dest": "/yew-scaffold/index.html"
    }
  ]
}